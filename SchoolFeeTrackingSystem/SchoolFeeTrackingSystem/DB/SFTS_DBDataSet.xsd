                    <CommandText>WITH CTE_STUDENT_FEE

AS 
(
SELECT *
FROM Student_Fee AS s
WHERE s.SY = @SY
),

CTE_SF_TOTAL
AS 
(
SELECT SUM(s.amount_req) as total
