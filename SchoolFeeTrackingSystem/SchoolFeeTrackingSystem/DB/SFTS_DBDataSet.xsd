FROM Guardian as g
INNER JOIN G_Trans_LOG as gt
ON g.guardian_ID = gt.guardian_ID
INNER JOIN SFTS_User as u
ON gt.[user_ID] = u.[user_ID]
WHERE (gt.guardian_ID = @Guardian_ID) AND (gt.SY = @SY)
),

A
